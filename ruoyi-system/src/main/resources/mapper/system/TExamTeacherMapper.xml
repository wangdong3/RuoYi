<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.TExamTeacherMapper">
    
    <resultMap type="TExamTeacher" id="TExamTeacherResult">
        <result property="connectId"    column="connect_id"    />
        <result property="examId"    column="exam_id"    />
        <result property="examName"    column="exam_name"    />
        <result property="userId"    column="user_id"    />
        <result property="userName"    column="user_name"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectTExamTeacherVo">
        select connect_id, exam_id, exam_name, user_id, user_name, create_by, create_time, update_by, update_time from t_exam_teacher
    </sql>

    <select id="selectTExamTeacherList" parameterType="TExamTeacher" resultMap="TExamTeacherResult">
        <include refid="selectTExamTeacherVo"/>
        <where>  
            <if test="examId != null "> and exam_id = #{examId}</if>
            <if test="examName != null  and examName != ''"> and exam_name like concat('%', #{examName}, '%')</if>
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="userName != null  and userName != ''"> and user_name like concat('%', #{userName}, '%')</if>
        </where>
    </select>
    
    <select id="selectTExamTeacherById" parameterType="Long" resultMap="TExamTeacherResult">
        <include refid="selectTExamTeacherVo"/>
        where connect_id = #{connectId}
    </select>
        
    <insert id="insertTExamTeacher" parameterType="TExamTeacher" useGeneratedKeys="true" keyProperty="connectId">
        insert into t_exam_teacher
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="examId != null">exam_id,</if>
            <if test="examName != null">exam_name,</if>
            <if test="userId != null">user_id,</if>
            <if test="userName != null">user_name,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="examId != null">#{examId},</if>
            <if test="examName != null">#{examName},</if>
            <if test="userId != null">#{userId},</if>
            <if test="userName != null">#{userName},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateTExamTeacher" parameterType="TExamTeacher">
        update t_exam_teacher
        <trim prefix="SET" suffixOverrides=",">
            <if test="examId != null">exam_id = #{examId},</if>
            <if test="examName != null">exam_name = #{examName},</if>
            <if test="userId != null">user_id = #{userId},</if>
            <if test="userName != null">user_name = #{userName},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where connect_id = #{connectId}
    </update>

    <delete id="deleteTExamTeacherById" parameterType="Long">
        delete from t_exam_teacher where connect_id = #{connectId}
    </delete>

    <delete id="deleteTExamTeacherByIds" parameterType="String">
        delete from t_exam_teacher where connect_id in 
        <foreach item="connectId" collection="array" open="(" separator="," close=")">
            #{connectId}
        </foreach>
    </delete>
    <delete id="deleteTExamTeacherExamId" parameterType="Long">
        delete from t_exam_teacher where exam_id = #{examId}
    </delete>
    <delete id="deleteTExamTeacherExamIds" parameterType="String">
        delete from t_exam_teacher where exam_id in
        <foreach item="examId" collection="array" open="(" separator="," close=")">
            #{examId}
        </foreach>
    </delete>

</mapper>